# Q3 - Shipping Priority Query
select
  l_orderkey,
  round(sum(l_extendedprice * (1 - l_discount)), 2) as revenue,
  o_orderdate,
  o_shippriority
from
  tpch_kudu.customer,
  tpch_kudu.orders,
  tpch_kudu.lineitem
where
  c_mktsegment = 'BUILDING'
  and c_custkey = o_custkey
  and l_orderkey = o_orderkey
  and o_orderdate < '1995-03-15'
  and l_shipdate > '1995-03-15'
group by
  l_orderkey,
  o_orderdate,
  o_shippriority
order by
  revenue desc,
  o_orderdate
limit 10
